{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "paths": {
      "@prismaClient": ["./prisma/generated/client"],
      "@prismaAuthClient": ["./prisma/generated/auth-client"],
      "@lib/*": ["./lib/*"],
      "@UI/*": ["./components/UI/*"],
      "components": ["./components/*"],
    },
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "target": "es2015",
    "downlevelIteration": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
